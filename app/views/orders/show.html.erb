<div class="container mt-3">
  <div class="text-center img-container-rocket">
    <h2>TO:DO PLACE IMAGES HERE</h2>
  </div>
  <div class="row">
    <div class="col-7"
      <p id="notice"><%= notice %></p>
      <div class="rocket-title">
        <h2>
          <strong>Your Booking:</strong>
          <%= @order.rocket.name %>
        </h2>
      </div>
    </p>

    <p>
      <strong>Details:</strong>
      <br>
      Start Date: <%= @order.start_date %>
      <br>
      End Date: <%= @order.end_date %>
      <br>
      Total Price: $<%= @order.total_price %>
      <br>
      Location: <%= @order.rocket.location %>
    </p>

    </div>
    <div class="col-5">
      <div class="show-card">
        <h2>Add a review!</h2>
        <%= render 'review-form', order: @order, review: @review %>
      </div>
    </div>
  </div>
  <div class="row mt-3">
    <div class="col-7 border-top">
      <div class="mt-3">
        <h2>Reviews</h2>
        <table class="table table-striped mt-3">
          <tr>
          </tr>
      </table>
      </div>
    </div>
  </div>
</div>


